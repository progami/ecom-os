    "build:analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next out dist",
    "post-build": "npm run db:generate",
    "start": "NODE_ENV=production node server.js",
    "start:prod": "NODE_ENV=production node --max-old-space-size=2048 server.js",
    "lint": "next lint",
    "lint:strict": "next lint",
    "test": "cd tests && playwright test e2e/",
    "test:headed": "cd tests && playwright test e2e/ --headed",
    "test:ui": "cd tests && playwright test --ui",
    "test:debug": "cd tests && playwright test --debug",
    "test:report": "cd tests && playwright show-report",
    "test:ci": "npm run lint && npm run type-check && npm run test",
