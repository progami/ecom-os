    "decimal.js": "^10.5.0",
    "formidable": "^3.5.4",
    "framer-motion": "^12.23.26",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.559.0",
    "next": "16.0.8",
    "next-auth": "5.0.0-beta.30",
    "open": "^10.1.2",
    "prisma": "6.19.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.68.0",
