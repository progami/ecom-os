    "uuid": "^11.1.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xero-node": "^11.2.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.4",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^11.0.1",
