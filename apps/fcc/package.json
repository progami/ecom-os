{
  "name": "@ecom-os/fcc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:server",
    "dev:server": "node --expose-gc server.js",
    "dev:enhanced": "node --expose-gc server.js",
    "dev:log": "node server-with-logging.js",
    "dev:http": "next dev -p 3003",
    "dev:https": "node server-https.js",
    "dev:server-only": "node --expose-gc server.js",
    "build": "next build",
    "start": "npm-run-all --parallel start:server workers",
    "start:server": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:populate-sops": "prisma db seed",
    "workers": "tsx start-workers.ts",
    "workers:dev": "tsx watch --ignore 'logs/**' --ignore 'prisma/dev.db*' start-workers.ts",
    "postinstall": "prisma generate",
    "type-check": "tsc --noEmit",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:coverage": "playwright test --reporter=html",
    "test:validation": "tsx test/master-test-suite.ts --validation-only",
    "test:integration": "tsx test/integration/dual-table-consolidation-test.ts",
    "test:performance": "tsx test/performance/precision-performance-test.ts",
    "test:regression": "tsx test/regression/cashflow-functionality-test.ts",
    "test:monitoring": "tsx test/monitoring/data-consistency-monitor.ts",
    "test:comprehensive": "tsx test/master-test-suite.ts",
    "test:quick": "tsx -e \"import { runQuickValidation } from './test/master-test-suite'; runQuickValidation().then(success => process.exit(success ? 0 : 1))\"",
    "test:pre-deploy": "tsx -e \"import { runPreDeploymentTests } from './test/master-test-suite'; runPreDeploymentTests().then(report => { console.log('System Health:', report.systemHealth); process.exit(report.summary.failedTests === 0 ? 0 : 1); })\"",
    "validate:pl": "tsx -e \"import { validatePLCalculations } from './test/validation/pl-calculation-validation'; validatePLCalculations().then(success => process.exit(success ? 0 : 1))\"",
    "validate:balance-sheet": "tsx -e \"import { validateBalanceSheet } from './test/validation/balance-sheet-validation'; validateBalanceSheet().then(success => process.exit(success ? 0 : 1))\"",
    "validate:invoice-counts": "tsx -e \"import { validateInvoiceBillCounts } from './test/validation/invoice-bill-counts-validation'; validateInvoiceBillCounts().then(success => process.exit(success ? 0 : 1))\"",
    "monitor:consistency": "tsx -e \"import { runConsistencyCheck } from './test/monitoring/data-consistency-monitor'; runConsistencyCheck().then(success => process.exit(success ? 0 : 1))\""
  },
  "dependencies": {
    "@ecom-os/auth": "workspace:*",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.11.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-table": "^8.13.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "bottleneck": "^2.19.5",
    "bullmq": "^5.54.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cookie": "^1.0.2",
    "csv-parse": "^5.6.0",
    "date-fns": "^3.3.1",
    "date-fns-tz": "^3.2.0",
    "decimal.js": "10.4.3",
    "formidable": "^3.5.4",
    "framer-motion": "^12.18.1",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.378.0",
    "next": "14.2.3",
    "next-auth": "^4.24.11",
    "open": "^10.1.2",
    "prisma": "^5.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.51.0",
    "react-hot-toast": "^2.5.2",
    "recharts": "^2.15.3",
    "redis": "^5.5.6",
    "sonner": "^2.0.5",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xero-node": "^11.2.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^11.0.1",
    "@playwright/test": "^1.53.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/swagger-ui-react": "^5.18.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.18",
    "critters": "^0.0.23",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "jsdom": "^26.1.0",
    "node-fetch": "^2.7.0",
    "node-forge": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.53.0",
    "postcss": "^8.4.35",
    "swagger-ui-react": "^5.24.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^3.2.3"
  }
}
