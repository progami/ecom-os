# AWS Configuration
aws_region = "us-east-1"

# Instance Configuration
instance_name    = "WMS-TEST-IaC"
instance_type    = "t3.micro"
key_name         = "wms-deploy-key"
root_volume_size = 30

# Environment
environment = "production"

# S3 Configuration
# Leave empty for auto-generated bucket name
s3_bucket_name = ""

# CORS allowed origins (update with your domain)
cors_allowed_origins = [
  "http://localhost:3000",
  "https://localhost:3000",
  "https://yourdomain.com"
]